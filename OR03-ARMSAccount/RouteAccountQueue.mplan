<?xml version='1.0' encoding='UTF-8'?>
<Mediator name="RouteAccountQueue" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.oracle.com/sca/1.0/mediator" wsdlTargetNamespace="http://xmlns.oracle.com/pcbpel/adapter/jms/ARIS-KPEI-PROJECT/OR03-ARMSAccount/Inbound_AccountQueue">
   <operation name="Consume_Message" deliveryPolicy="AllOrNothing" priority="4" validateSchema="false">
      <switch>
         <case executionType="direct" name="PrepareAccountAck.prepareaccountack_client.process">
            <action>
               <transform>
                  <part name="$out.payload" function="xslt(xsl/AccountQ_To_AccountProcess.xsl, $in.body)"/>
               </transform>
               <invoke reference="PrepareAccountAck.prepareaccountack_client" operation="process">
                  <onReply>
                     <transform>
                        <part name="$out.part1" function="xslt(xsl/ResponseMessage_To_ResponseMessage.xsl, $in.payload)"/>
                     </transform>
                     <forward reference="ResponseMessageSender" operation="execute"/>
                  </onReply>
               </invoke>
            </action>
         </case>
      </switch>
   </operation>
</Mediator>
